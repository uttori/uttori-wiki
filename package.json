  "files": [
    "dist/*",
    "src/*"
  ],
  "module": "src/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/index.js",
      "types": "./dist/index.d.ts"
    },
    "./config": {
      "import": "./src/config.js",
