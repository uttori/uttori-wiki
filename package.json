{
  "name": "uttori-wiki",
  "version": "1.13.2-pre",
  "description": "Uttori Wiki is a wiki functionality module for Uttori.",
  "author": "Matthew Callis <matthew.callis@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "repository": "uttori/uttori-wiki",
  "homepage": "https://github.com/uttori/uttori-wiki",
  "keywords": [
    "wiki"
  ],
  "engines": {
    "node": ">= 8.11"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "fs-extra": "^7.0.1",
    "ramda": "^0.26.1",
    "request": "^2.88.0",
    "slugify": "^1.3.4",
    "uttori-document": "^1.2.0"
  },
  "optionalDependencies": {
    "express-recaptcha": "^4.1.1"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "body-parser": "^1.18.3",
    "ejs": "^2.6.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-ava": "^6.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-no-inferred-method-name": "^1.0.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-xss": "^0.1.9",
    "express": "^4.16.4",
    "express-ejs-layouts": "^2.5.0",
    "fs-extra-debug": "^1.0.4",
    "markdown-it": "^8.4.2",
    "nyc": "^13.3.0",
    "sinon": "^7.3.1",
    "supertest": "^4.0.2",
    "uttori-storage-provider-json-file": "^2.1.1",
    "uttori-upload-provider-multer": "^1.2.1"
  },
  "files": [
    "app/*"
  ],
  "main": "app/index.js",
  "nyc": {
    "cache": true,
    "all": true,
    "check-coverage": true,
    "include": [
      "app/**/*.js"
    ],
    "exclude": []
  },
  "scripts": {
    "lint": "eslint .",
    "report": "nyc report --reporter=html",
    "pretest": "killall -SIGINT uttori-wiki-test-server && node test/_helpers/server.js uttori-wiki-test-server &",
    "posttest": "killall -SIGINT uttori-wiki-test-server || true",
    "test": "NODE_ENV=test nyc ava --serial",
    "test-debug": "DEBUG=Uttori* NODE_ENV=test nyc ava --serial",
    "validate": "npm ls"
  }
}
