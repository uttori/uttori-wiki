{
  "name": "@uttori/wiki",
  "version": "3.4.0",
  "description": "Uttori Wiki is a wiki functionality module for Uttori.",
  "author": "Matthew Callis <matthew.callis@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uttori/uttori-wiki.git"
  },
  "bugs": {
    "url": "https://github.com/uttori/uttori-wiki/issues"
  },
  "homepage": "https://github.com/uttori/uttori-wiki",
  "keywords": [
    "uttori",
    "wiki"
  ],
  "engines": {
    "node": ">= 12"
  },
  "dependencies": {
    "@uttori/event-dispatcher": "^1.0.3",
    "debug": "^4.2.0",
    "ejs": "^3.1.5",
    "express-ejs-layouts": "^2.5.0",
    "ramda": "^0.27.1",
    "uttori-document": "^1.4.1"
  },
  "optionalDependencies": {},
  "devDependencies": {
    "@uttori/search-provider-lunr": "^3.3.3",
    "@uttori/storage-provider-json-memory": "^3.4.2",
    "ava": "^3.13.0",
    "body-parser": "^1.19.0",
    "coveralls": "^3.1.0",
    "docsify-cli": "^4.4.1",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-ava": "^11.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-inferred-method-name": "^2.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^23.0.0",
    "eslint-plugin-xss": "^0.1.10",
    "express": "^4.17.1",
    "jsdoc": "^3.6.6",
    "jsdoc-to-markdown": "^6.0.1",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "release-it": "^14.2.1",
    "sinon": "^9.2.1",
    "supertest": "^6.0.1",
    "typescript": "^4.0.5"
  },
  "files": [
    "src/*",
    "types/*"
  ],
  "main": "src/index.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "ignoredByWatcher": [
      "!src/**/*.js"
    ],
    "concurrency": 10,
    "failFast": false,
    "tap": false,
    "verbose": true
  },
  "nyc": {
    "cache": true,
    "all": true,
    "check-coverage": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": []
  },
  "scripts": {
    "docs": "jsdoc2md --private --example-lang js --template rm.hbs src/wiki.js > README.md; jsdoc2md --private --example-lang js src/wiki.js > docs/wiki.md;",
    "docs-serve": "docsify init ./docs && docsify serve ./docs",
    "lint": "eslint .",
    "make-types": "rm -fr types/ && mkdir types && tsc --project .tsconfig.json",
    "release": "release-it",
    "report": "nyc report --reporter=html",
    "pretest": "killall -SIGINT uttori-wiki-test-server && node test/_helpers/server.js uttori-wiki-test-server &",
    "posttest": "killall -SIGINT uttori-wiki-test-server || true",
    "test": "NODE_ENV=test nyc ava",
    "test-debug": "DEBUG=Uttori* NODE_ENV=test ava --serial --concurrency=1",
    "validate": "npm ls"
  },
  "directories": {
    "test": "test"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
