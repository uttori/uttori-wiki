{
  "name": "@uttori/wiki",
  "version": "6.0.0",
  "description": "Uttori Wiki is a wiki functionality module for Uttori.",
  "author": "Matthew Callis <matthew.callis@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uttori/uttori-wiki.git"
  },
  "bugs": {
    "url": "https://github.com/uttori/uttori-wiki/issues"
  },
  "homepage": "https://github.com/uttori/uttori-wiki",
  "keywords": [
    "uttori",
    "wiki",
    "knowledge base",
    "knowledge management",
    "note taking"
  ],
  "engines": {
    "node": ">= 20"
  },
  "dependencies": {
    "@uttori/event-dispatcher": "^2.0.2"
  },
  "optionalDependencies": {
    "debug": "^4.3.4",
    "express-session": "^1.17.3"
  },
  "peerDependencies": {
    "express": "^4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@uttori/search-provider-lunr": "^3.4.0",
    "@uttori/storage-provider-json-memory": "^5",
    "ava": "^6.0.1",
    "c8": "^8.0.1",
    "cors": "^2.8.5",
    "ejs": "^3.1.9",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-anti-trojan-source": "^1.1.1",
    "eslint-plugin-ava": "^14.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^46.9.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.5.0",
    "eslint-plugin-no-inferred-method-name": "^2.0.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^2.1.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-xss": "^0.1.12",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0",
    "jsdoc-tsimport-plugin": "^1.0.5",
    "pre-commit": "^1.2.2",
    "release-it": "^17.0.1",
    "sinon": "^17.0.1",
    "supertest": "^6.3.3",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "module": "src/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/index.js"
    },
    "./config": {
      "import": "./src/index.js"
    },
    "./middleware": {
      "import": "./src/middleware.js"
    },
    "./wiki": {
      "import": "./src/wiki.js"
    }
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "concurrency": 10,
    "failFast": false,
    "tap": false,
    "verbose": true
  },
  "c8": {
    "clean": true,
    "per-file": true,
    "check-coverage": false,
    "include": [
      "src/**/*.js"
    ],
    "exclude": []
  },
  "release-it": {
    "github": {
      "release": true
    }
  },
  "scripts": {
    "docs": "jsdoc2md --configure ./jsdoc.conf.json --private --example-lang js --template rm.hbs src/wiki.js > README.md; jsdoc2md --configure ./jsdoc.conf.json --private --example-lang js src/wiki.js > docs/wiki.md; jsdoc2md --configure ./jsdoc.conf.json --private --example-lang js src/config.js > docs/config.md; jsdoc2md --configure ./jsdoc.conf.json --private --example-lang js src/plugins/add-query-output.js > docs/plugins/add-query-output.md",
    "lint": "eslint --ignore-pattern \"/test/\" . ",
    "make-types": "tsc --module nodenext --project ./tsconfig.json",
    "release": "release-it",
    "pretest": "killall -SIGINT uttori-wiki-test-server && node test/_helpers/server.js uttori-wiki-test-server &",
    "posttest": "killall -SIGINT uttori-wiki-test-server || true",
    "test": "NODE_ENV=test c8 ava",
    "test-debug": "DEBUG=Uttori* NODE_ENV=test ava --serial --concurrency=1",
    "validate": "npm ls"
  },
  "directories": {
    "test": "test"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
