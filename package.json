{
  "name": "uttori-wiki",
  "version": "2.3.1-pre",
  "description": "Uttori Wiki is a wiki functionality module for Uttori.",
  "author": "Matthew Callis <matthew.callis@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "repository": "uttori/uttori-wiki",
  "homepage": "https://github.com/uttori/uttori-wiki",
  "keywords": [
    "wiki"
  ],
  "engines": {
    "node": ">= 10.15.3"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "fs-extra": "^8.1.0",
    "ramda": "^0.26.1",
    "uttori-document": "^1.4.0"
  },
  "optionalDependencies": {},
  "devDependencies": {
    "ava": "^2.3.0",
    "body-parser": "^1.19.0",
    "ejs": "^2.6.2",
    "eslint": "^5",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-ava": "^8.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-no-inferred-method-name": "^1.0.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-optimize-regex": "^1.1.6",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^10.0.0",
    "eslint-plugin-xss": "^0.1.9",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "fs-extra-debug": "^1.0.4",
    "markdown-it": "^9.1.0",
    "nyc": "^14.1.1",
    "sinon": "^7.4.1",
    "supertest": "^4.0.2",
    "uttori-analytics-provider-json-file": "^2.0.0",
    "uttori-storage-provider-json-file": "^2.2.3",
    "uttori-upload-provider-multer": "^1.2.4"
  },
  "files": [
    "app/*"
  ],
  "main": "app/index.js",
  "nyc": {
    "cache": true,
    "all": true,
    "check-coverage": true,
    "include": [
      "app/**/*.js"
    ],
    "exclude": []
  },
  "scripts": {
    "lint": "eslint .",
    "report": "nyc report --reporter=html",
    "pretest": "killall -SIGINT uttori-wiki-test-server && node test/_helpers/server.js uttori-wiki-test-server &",
    "posttest": "killall -SIGINT uttori-wiki-test-server || true",
    "test": "NODE_ENV=test nyc ava --serial",
    "test-debug": "DEBUG=Uttori* NODE_ENV=test ava --serial",
    "validate": "npm ls"
  }
}
